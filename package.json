{
  "name": "@kloudless/file-picker",
  "version": "2.6.5",
  "author": "Kloudless <hello@kloudless.com> (https://kloudless.com)",
  "description": "The Kloudless File Picker is a JavaScript library that allows your users to browse and select files and folders from their storage services. It comes in two modes: a chooser and a saver.",
  "main": "./dist/commonjs2/loader.min.js",
  "types": "./dist/commonjs2/loader.d.ts",
  "keywords": [
    "kloudless",
    "file picker",
    "file explorer",
    "file picker",
    "cloud storage"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/kloudless/file-picker.git"
  },
  "bugs": {
    "url": "https://github.com/kloudless/file-picker/issues",
    "email": "support@kloudless.com"
  },
  "homepage": "https://kloudless.com/products/file-picker",
  "license": "(MIT OR AGPL-3.0-only)",
  "scripts": {
    "dev": "NODE_ENV=development node ./dev-server/dev-server.js",
    "dev:story": "NODE_ENV=development node ./config/story-dev-server.js",
    "install-deps": "bower install",
    "storybook:test": "yarn --cwd ./storybook-test/ run storybook",
    "storybook:vue": "yarn --cwd ./storybook-vue/ run storybook",
    "storybook:react": "yarn --cwd ./storybook-react/ run storybook",
    "build:storybook": "yarn --cwd ./storybook-vue/ run build && yarn --cwd ./storybook-react run build",
    "deploy:storybook": "yarn run build:storybook && storybook-to-ghpages --remote=github --existing-output-dir=.demo",
    "_build": "rm -rf $DIST_FOLDERS && node config/build.js config/webpack.$WEBPACK_CONFIG.conf.js",
    "build": "NODE_ENV=production DIST_FOLDERS='dist/commonjs2 dist/loader dist/picker dist/explorer test/dist' WEBPACK_CONFIG=prod yarn run _build",
    "build:agpl": "BUILD_LICENSE=AGPL yarn run build",
    "build:dev": "DIST_FOLDERS=dev NODE_ENV=development WEBPACK_CONFIG=story yarn run _build",
    "build:template": "pug src/picker/templates/index.pug -o dist/template -s && ejs-cli template/picker.ejs > dist/custom-index.html && rm -rf dist/template",
    "prepack": "bash generate_npmignore.sh && npm ci && yarn run build",
    "postpack": "rm -f .npmignore",
    "dist-test": "node ./test/dist-test-server.js",
    "test:ts": "eslint --config .eslintrc-ts.js --ext .ts . && tsc test/ts/*.ts --noEmit && echo 'âœ…  Passed type tests.'",
    "stylelint": "stylelint ./src/**/*.less --syntax less",
    "stylelint:fix": "yarn run stylelint -- --fix",
    "lint": "eslint ."
  },
  "dependencies": {
    "hoist-non-react-statics": "3.3.2",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/cli": "7.26.4",
    "@babel/core": "7.26.9",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-react": "7.26.3",
    "@kloudless/file-picker-plupload-module": "https://github.com/Webinfinity/file-picker-plupload-module#ea5957be8c0c82816ab386fe9ff891d35ccd1fc2",
    "@storybook/storybook-deployer": "2.8.1",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "autoprefixer": "9.8.8",
    "babel-loader": "^8.4.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-define": "1.3.2",
    "bower": "1.8.14",
    "cldr-data-downloader": "1.1.0",
    "compare-versions": "3.6.0",
    "copy-webpack-plugin": "5.1.2",
    "core-js": "3.40.0",
    "css-loader": "3.6.0",
    "cssnano": "4.1.11",
    "ejs": "2.7.4",
    "ejs-cli": "2.2.3",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.4",
    "expose-loader": "0.7.5",
    "express": "4.21.2",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "3.2.0",
    "izitoast": "1.4.0",
    "jquery": "2.2.4",
    "knockout": "^3.5.1",
    "less": "3.13.1",
    "less-loader": "5.0.0",
    "lodash.debounce": "4.0.8",
    "material-icons-font": "2.1.0",
    "mini-css-extract-plugin": "0.7.0",
    "moment": "^2.30.1",
    "morgan": "1.10.0",
    "normalize.css": "8.0.1",
    "null-loader": "^4.0.1",
    "postcss-loader": "3.0.0",
    "pug": "2.0.4",
    "pug-cli": "1.0.0-alpha6",
    "pug-loader": "2.4.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "regenerator-runtime": "0.13.11",
    "sammy": "0.7.6",
    "script-ext-html-webpack-plugin": "2.1.5",
    "style-loader": "1.3.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.1.1",
    "typescript": "3.9.10",
    "url-loader": "4.1.1",
    "webpack": "4.47.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-middleware": "3.7.3",
    "webpack-hot-middleware": "2.26.1",
    "webpack-merge": "4.2.2"
  },
  "packageManager": "yarn@4.6.0"
}
